{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raydeel",
  "title": "RayDeel",
  "description": "the raycast extention for tabdeel cryptocurrency exchange",
  "icon": "command-icon.png",
  "owner": "amin-team",
  "categories": [
    "Finance"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Tabdeel",
      "subtitle": "Shows your tabdeel wallet",
      "description": "Quickly see your tabdeel wallet",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "api_key",
      "description": "required for fetching data, you can find it in here: https://tabdeal.org/panel/settings/api",
      "type": "textfield",
      "required": true,
      "title": "API KEY"
    },
    {
      "name": "api_secret",
      "description": "required for fetching data, you can find it in here: https://tabdeal.org/panel/settings/api",
      "type": "password",
      "required": true,
      "title": "API SECRET"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "axios": "^1.3.3",
    "axios-retry": "^3.4.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "crypto-js": "^4.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "version": "0.0.0"
}
